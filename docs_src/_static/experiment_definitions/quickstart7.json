{
  "experiment_definition": {
    "target": "Sales",
    "link_function": "log",
    "target_dimensions": ["Store"],
    "time_index": "Date",
    "data_path": "divina://retail_sales",
    "include_features": ["Store", "Weekday", "Month", "WeekOfYear",
       "Holiday", "HolidayType", "StoreType", "Assortment", "LastDayOfMonth", "Promo"],
    "forecast_end": "01-01-2016",
    "frequency": "D",
    "encode_features": ["Store", "Month", "WeekOfYear", "StoreType", "Weekday", "HolidayType", "Assortment", "Promo"],
    "joins": [
                {
                    "data_path": "divina://time",
                    "join_on": ["Date", "Date"],
                    "as": "time"
                }
            ],
    "bin_features": {"Month": [3, 6, 9]},
    "time_validation_splits": ["2015-07-18"],
    "scenarios": {
      "Promo": {
        "mode": "constant",
        "constant_values": [
          0,
          1
        ]
      },
      "StoreType": {
        "mode": "last"
      },
      "Assortment": {
        "mode": "last"
      }
    }
  }
}