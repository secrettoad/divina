{
  "experiment_definition": {
    "target": "Sales",
    "link_function": "log",
    "target_dimensions": [
      "Store"
    ],
    "time_index": "Date",
    "data_path": "divina://retail_sales",
    "include_features": [
      "Store",
      "Weekday",
      "Promo",
      "Month",
      "WeekOfYear",
      "Holiday",
      "HolidayType",
      "StoreType",
      "Assortment"
    ],
    "joins": [
      {
        "data_path": "divina://time",
        "join_on": [
          "Date",
          "Date"
        ],
        "as": "time"
      }
    ],
    "forecast_end": "01-01-2016",
    "frequency": "D",
    "encode_features": [
      "Store",
      "Month",
      "WeekOfYear",
      "StoreType",
      "Weekday",
      "HolidayType",
      "Assortment"
    ],
    "confidence_intervals": [
      0,
      100
    ],
    "bin_features": {
      "Month": [
        3,
        6,
        9
      ]
    },
    "scenarios": {
      "Promo": {
        "mode": "constant",
        "constant_values": [
          0,
          1
        ]
      },
      "StoreType": {
        "mode": "last"
      },
      "Assortment": {
        "mode": "last"
      }
    }
  }
}